        <div class="carousel-inner twitter">

          <div class="item active">
            <div class="row" id="carousel-row">
            <% $twitter_client.search("cat filter:media ", result_type: "popular", lang: "en").take(8).each.with_index do |tweet, index| %>
              <% if (index + 1) > 3 && (index % 3) == 0 %>
                  </div>
                </div>
                <div class="item">
                  <div class="row" id="carousel-row">
              <% end %>
                <div class="col-md-4">
                  <div class="row twitter-box">
                    <div class="col-md-4 col-xs-4">
                      <%= image_tag tweet.user.profile_image_uri, class: "twitter-image" %>
                    </div>
                    <div class="col-md-6 col-xs-6 text-left">
                      <p class="twitter-tittle">
                        <%= tweet.user.name %>
                      </p>
                    </div>
                    <div class="col-md-12 col-xs-12">
                      <%= tweet.text %>
                    </div>
                  </div>
                </div>
            <% end %>
