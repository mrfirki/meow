                    <iframe width="640" height="350" src="https://youtube.com/embed/<%= result.id.videoId %>" allowfullscreen="allowfullscreen"></iframe>
                      </div>
                    </div>
                  <% when 'youtube#channel' %>
                    <p><%= result.snippet.title %> <%= result.id.channelId %></p>
